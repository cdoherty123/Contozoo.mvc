@model IEnumerable<Contozoo.mvc.Models.AnimalModel>

@{
ViewData["Title"] = "Meet Our Animals";
}



<div class="text-center">
<h1 class="display-4">Meet Our Animals: </h1>
<br />
<h2 class=""text-centre" style="padding-bottom:20px">Here is a list of all the animals you can visit at Contozoo!</h2>
<table class="table">
<thead>
<tr>
<th>
@Html.DisplayNameFor(model => model.AnimalId)
</th>
<th>
@Html.DisplayNameFor(model => model.CAI)
</th>
<th>
@Html.DisplayNameFor(model => model.Name)
</th>
<th>
@Html.DisplayNameFor(model => model.Location)
</th>
<th>
@Html.DisplayNameFor(model => model.LunchHour)
</th>
<th>
@Html.DisplayNameFor(model => model.Notes)
</th>
</tr>
</thead>
<tbody>
@foreach (var item in Model)
{
<tr>
<td>
@Html.DisplayFor(modelItem => item.AnimalId)
</td>
<td>
@Html.DisplayFor(modelItem => item.CAI)
</td>
<td>
@Html.DisplayFor(modelItem => item.Name)
</td>
<td>
@Html.DisplayFor(modelItem => item.Location)
</td>
<td>
@Html.DisplayFor(modelItem => item.LunchHour)
</td>
<td>
@Html.DisplayFor(modelItem => item.Notes)
</td>
</tr>
}
</tbody>
</table>



</div>
